{
  "name": "ts-prismic",
  "version": "0.2.1",
  "description": "A light-weight interface to Prismic's REST API",
  "license": "MIT",
  "main": "./dist/ts-prismic.js",
  "module": "./dist/ts-prismic.modern.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "microbundle build src/index.ts -f modern,cjs --tsconfig tsconfig.build.json",
    "dev": "microbundle watch src/index.ts -f modern,cjs --tsconfig tsconfig.build.json",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "jest",
    "prepare": "yarn build",
    "release": "standard-version"
  },
  "author": "Angelo Ashmore",
  "homepage": "https://github.com/angeloashmore/ts-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/ts-prismic.git"
  },
  "keywords": [
    "prismic",
    "rest"
  ],
  "dependencies": {
    "type-fest": "^0.20.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "printWidth": 80
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
